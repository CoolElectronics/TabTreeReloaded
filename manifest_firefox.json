{
  "manifest_version": 2,
  "name": "TabTree Reloaded",
  "description": "A feature-packed vertical tab tree for power users.",
  "version": "2.1.4",

  "background": {
    "scripts": [
      "generated/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "32": "img/icon32.png"
  },
  "browser_action": {},

  "permissions": [
    "storage",
    "tabs",
    "identity",
    "contextMenus",
    "unlimitedStorage"
  ]
}
